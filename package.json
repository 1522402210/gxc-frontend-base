{
  "name": "gxc-frontend-base",
  "author": "liuzongyuan@gxb.io",
  "version": "0.1.0",
  "description": "gxc frontend base repo, inlcudes common util, rules, style...",
  "main": "build/index.js",
  "keywords": [
    "es6",
    "babel",
    "eslint",
    "npm package"
  ],
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "build": "npm run build:script && npm run build:style",
    "build:style": "sass src/style/:build/style/",
    "build:script": "babel src --out-dir build --ignore style",
    "clean": "rm -rf build",
    "lint": "eslint src test"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.1.2",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-react": "7.10.0",
    "mocha": "5.2.0",
    "sass": "^1.10.4"
  },
  "license": "MIT"
}
